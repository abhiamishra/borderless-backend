{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "functions": {
        "gemini.py": {
            "maxDuration": 60
        },
        "main.py": {
            "maxDuration": 60
        }
    }
  }

